{
  "expo": {
    "name": "stranger-detective",
    "slug": "stranger-detective",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splashart.png",
      "resizeMode": "cover",
      "backgroundColor": "#2f3f56"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gabirel.strangerdetective"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
